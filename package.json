{
    "name": "progen-extension",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.33.1"
    },
    "main": "./extension.js",
    "activationEvents": [
        "*"
    ],
    "contributes": {
        "commands": [
            {
                "command": "progen.run",
                "title": "Progen: run"
            },
            {
                "command": "progen.export",
                "title": "Progen: export"
            },
            {
                "command": "progen.config",
                "title": "Progen: config"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "progen-view",
                    "title": "progen",
                    "icon": "./logo.png"
                }
            ]
        },
        "views": {
            "progen-view": [
                {
                    "id": "progen",
                    "name": "progen"
                }
            ]
        },
        "configuration": {
            "type": "object",
            "title": "Extension Configuration",
            "properties": {
                "progen.username": {
                    "type": "string",
                    "default": "ps00",
                    "description": "ユーザー名"
                }
            }
        }
    }
}